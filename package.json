{
  "version": "0.1.3",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@annikenyt/sdui-sdk-js",
  "author": "AnnikenYT",
  "module": "dist/sdui-sdk-js.esm.js",
  "size-limit": [
    {
      "path": "dist/sdui-sdk-js.cjs.production.min.js",
      "limit": "15 KB"
    },
    {
      "path": "dist/sdui-sdk-js.esm.js",
      "limit": "15 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^0.26.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnnikenYT/sdui-sdk-js.git"
  }
}
